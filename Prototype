Option Explicit
Public Function doALl()
    Call addValtoM
    'Call rowI'
    Call descriptionFixLtoO
End Function


Public Function addValtoM()
    Dim i As Integer, sheetEnd As Integer, eVal As String, eValNum As Long
    
    sheetEnd = Range("A1").EntireColumn.Rows.Count - Range("A65536", Range("a65536").End(xlUp)).Rows.Count + 1
    
    For i = 1 To sheetEnd Step 1
        eVal = Range("A1").Offset(i, 10).Value
        If (IsNumeric(eVal)) Then
            eValNum = CLng(eVal)
            Range("A1").Offset(i, 12).Value = (eValNum * 10) + RandomNumbers(7, 12)
        End If
    Next i
End Function

